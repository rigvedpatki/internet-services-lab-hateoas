<h4 {{action "toggleVisibility" model}}>< span class="caret {{if model.visible "caret-down" "caret-right"}}"> </span> {{model.name}}</h4>
{{#if model.visible}}
  <form {{action "save" model on="change"}}>
    Name:{{input value=model.name}}
    Type:{{#x-select value=model.type}}
      {{#each types as |type|}}
        {{#x-option selected=(if (eq model.type type) "selected" "") value=type}}{{type}}{{/x-option}}
      {{/each}}
    {{/x-select}}
    Description:{{input value=model.description}}
 
  </form>

  <form id="dd"{{action "save" model on="change"}}>
   
    Example:{{input value=model.example}}
  </form>
  <div id="res">
  {{#link-to "queryParameters.delete" model tagName="button" class="btn btn-danger btn-xs"}}Remove{{/link-to}}
</div>
{{/if}}
