{{#if model.id}}
<div>
  <div>
    <h3>API URL</h3>
    <div class="jumbotron">
      <form {{action "save" model on="change" }}>
        <h4 id="ff">Protocol:
        {{#x-select value=model.protocol action="saveSelection"}}
        {{#x-option value='' selected="selected"}}{{/x-option}}
        {{#each protos as |proto|}}
        {{#x-option value=proto}}{{proto}}{{/x-option}}
        {{/each}}
        {{/x-select}}<br></h4>
       <h4 id="f1"> Port: {{input value=model.port}}</h4>
        <br>
        <h4 id="f2">Domain: {{input value=model.domain}}</h4>
        <br>
        <h4> Base Path: {{input value=model.base_path }}</h4>
        <br>
        <h4 id="f3">Endpoint URL: {{input value=model.endpoint_url readonly=true}}</h4>
        <br>
      </form>
      <div id="res4">
        {{#link-to "apiurls.delete" model tagName="button" class="btn btn-danger btn-xs"}}Remove{{/link-to}}
      </div>
    </div>
  </div>
  <div>
    <h3> General Information </h3>

  </div>
  <div>
    <h3> Authentication Protocol</h3>

  </div>
</div>

{{/if}}
